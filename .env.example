# Example Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration (future use)
# REACT_APP_API_URL=http://localhost:3001
# REACT_APP_API_KEY=your_api_key_here

# Processing Mode (default: cloud)
# REACT_APP_DEFAULT_PROCESSING_MODE=cloud

# Feature Flags (future use)
# REACT_APP_ENABLE_AUTH=false
# REACT_APP_ENABLE_ANALYTICS=false
# REACT_APP_ENABLE_INTEGRATIONS=false

# LLM Configuration (future use)
# REACT_APP_LLM_PROVIDER=openai
# REACT_APP_LLM_MODEL=gpt-4

# Privacy & Data
# REACT_APP_PRIVACY_MODE=strict
# REACT_APP_DATA_RETENTION_DAYS=90
